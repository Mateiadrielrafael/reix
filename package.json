{
    "name": "root",
    "private": true,
    "devDependencies": {
        "@babel/core": "^7.6.0",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/preset-typescript": "^7.6.0",
        "@types/jest": "^24.0.18",
        "cross-env": "^6.0.0",
        "jest": "^24.9.0",
        "lerna": "^3.16.4",
        "rimraf": "^3.0.0",
        "rollup": "^1.21.4",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-terser": "^5.1.2",
        "ts-jest": "^24.1.0"
    },
    "scripts": {
        "prebuild": "lerna exec rimraf dist",
        "build": "lerna exec -- rollup -c ../../rollup.config.js",
        "build:watch": "lerna exec --parallel -- rollup -c ../../rollup.config.js -w",
        "test:all": "lerna exec -- jest -c=../../jest.config.js",
        "test:changed": "lerna exec --since master -- test -c=../../jest.config.js",
        "predeploy": "npm run build",
        "deploy": "lerna publish"
    },
    "dependencies": {}
}
