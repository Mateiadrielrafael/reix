{
    "name": "reix",
    "private": true,
    "devDependencies": {
        "@babel/core": "^7.6.2",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
        "@types/chai": "^4.2.3",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.7.8",
        "@types/sinon": "^7.0.13",
        "chai": "^4.2.0",
        "cross-env": "^6.0.0",
        "lerna": "^3.16.4",
        "mocha": "^6.2.0",
        "rimraf": "^3.0.0",
        "rollup": "^1.21.4",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-dts": "^1.1.7",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-terser": "^5.1.2",
        "rollup-plugin-typescript2": "^0.24.2",
        "sinon": "^7.5.0",
        "ts-node": "^8.4.1",
        "typescript": "^3.6.3"
    },
    "scripts": {
        "prebuild": "lerna exec rimraf dist",
        "build": "lerna exec --since -- rollup -c ../../rollup.config.js",
        "build:watch": "lerna exec --parallel -- rollup -c ../../rollup.config.js -w",
        "test": "lerna exec -- mocha --opts ../../test/mocha.opts",
        "test:changed": "lerna exec --since -- mocha --opts ../../test/mocha.opts",
        "predeploy": "npm run build",
        "deploy": "lerna publish",
        "setup:ci": "npm ci && lerna bootstrap"
    },
    "dependencies": {}
}