{
    "name": "reix",
    "private": true,
    "devDependencies": {
        "@babel/core": "^7.6.2",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
        "@babel/preset-env": "^7.6.2",
        "@types/babel__core": "^7.1.3",
        "@types/chai": "^4.2.3",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.7.8",
        "@types/sinon": "^7.0.13",
        "chai": "^4.2.0",
        "cross-env": "^6.0.0",
        "lerna": "^3.16.4",
        "mocha": "^6.2.0",
        "nano-server": "^0.5.4",
        "nodemon": "^1.19.3",
        "npm-run-all": "^4.1.5",
        "patch-package": "^6.2.0",
        "rimraf": "^3.0.0",
        "rollup": "^1.21.4",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-dts": "^1.1.7",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-terser": "^5.1.2",
        "rollup-plugin-typescript2": "^0.24.2",
        "sinon": "^7.5.0",
        "ts-node": "^8.4.1",
        "typedoc": "^0.15.0",
        "typedoc-neo-theme": "^1.0.2",
        "typedoc-plugin-external-module-map": "^1.0.0",
        "typedoc-plugin-markdown-pages": "^0.3.0",
        "typescript": "^3.6.3"
    },
    "scripts": {
        "clean": "lerna exec rimraf dist",
        "prebuild": "npm run clean",
        "build": "lerna exec --since -- rollup -c ../../rollup.config.js",
        "prebuild:all": "npm run clean",
        "build:all": "lerna exec -- rollup -c ../../rollup.config.js",
        "test": "lerna exec -- mocha --opts ../../test/mocha.opts",
        "test:root": "mocha --opts ./test/mocha.root.opts",
        "test:changed": "lerna exec --since -- mocha --opts ../../test/mocha.opts",
        "predeploy": "npm run build && npm run docs",
        "deploy": "lerna publish",
        "setup:ci": "npm ci && lerna bootstrap && patch-package",
        "predocs": "rimraf _docs",
        "docs": "typedoc",
        "docs:watch": "nodemon --config ./nodemon.docs.json",
        "docs:serve": "nano-server 8080 _docs",
        "docs:dev": "run-p docs:watch docs:serve"
    },
    "dependencies": {}
}
